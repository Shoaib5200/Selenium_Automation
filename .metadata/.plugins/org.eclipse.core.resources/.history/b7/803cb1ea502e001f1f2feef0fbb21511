import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

public class Assignment {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		System.setProperty("webdriver.chrome.driver", "C://SHOAIB DANISH//Study Material//Automation//chromedriver-win64//chromedriver.exe");
		WebDriver driver = new ChromeDriver();
		driver.get("https://rahulshettyacademy.com/AutomationPractice/");
		
		// 1. Check the checkbox
		// input[type='checkbox']
		
		driver.findElement(By.xpath("//input[@value='option3']")).click();
		System.out.println(driver.findElement(By.xpath("//input[@value='option3']")).getText());
		
		
		

	}

}
